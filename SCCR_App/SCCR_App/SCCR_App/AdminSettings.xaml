<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SCCR_App.AdminSettings">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Hey Admin!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />   
            <Button Text="Create a new Rescue Profile"
                VerticalOptions="CenterAndExpand"
                HorizontalOptions="CenterAndExpand"
                Clicked="NewRescueProfileClicked" />
            <Button Text="Log Out"
                VerticalOptions="End"
                HorizontalOptions="CenterAndExpand"
                Clicked="AdminLogOutClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>